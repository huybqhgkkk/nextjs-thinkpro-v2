{
  "name": "boilerplate-nextjs-antd-less",
  "version": "3.0.0",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-thunk, antd, less",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "NODE_ENV=development env-cmd next",
    "export": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next export",
    "build": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next build",
    "build:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next build",
    "start": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next start",
    "start:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next start",
    "lint": "next lint",
    "analyze": "NODE_ENV=production ANALYZE=true env-cmd -f .env.production --fallback --silent next build",
    "serve": "serve .next/analyze",
    "start:pm2": "NODE_ENV=production pm2 start npm --name \"boilerplate\" -- start"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.5",
    "@ant-design/pro-descriptions": "^1.9.0",
    "@ant-design/pro-form": "^1.43.0",
    "@ant-design/pro-layout": "^6.26.0",
    "@ant-design/pro-table": "^2.56.0",
    "ant-design": "^1.0.0",
    "antd": "^4.23.6",
    "chalk": "^5.0.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "frontmatter-markdown-loader": "^3.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "12.3.1",
    "next-compose-plugins": "^2.2.1",
    "next-redux-wrapper": "^8.0.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "rc-picker": "^2.6.11",
    "react": "18.2.0",
    "react-cookies": "^0.1.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-redux": "^7.2.6",
    "react-use": "^17.4.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.4.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@next/bundle-analyzer": "^12.3.1",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.5",
    "env-cmd": "^10.1.0",
    "eslint": "<8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less-vars-to-js": "^1.3.0",
    "next-plugin-antd-less": "^1.8.0",
    "webpack": "^5.74.0"
  }
}
